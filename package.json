{
  "name": "gist-library",
  "version": "1.0.0",
  "description": "An application to organize Github Gists",
  "main": "index.js",
  "scripts": {
    "elm-live": "elm-live lib/Main.elm --output=dist/elm.js --open",
    "sass": "node-sass --output-style compressed -o 'dist' 'assets/css/styles.scss'",
    "autoprefixer": "postcss dist/styles.css -r -m -u autoprefixer --autoprefixer.browsers '> 1%'",
    "css": "npm run sass && npm run autoprefixer",
    "imagemin": "imagemin assets/images/* dist/images",
    "prod": "elm-make lib/Main.elm --output=dist/elm.js"
  },
  "author": "Nathan Kleekamp",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "elm": "^0.18.0",
    "elm-live": "^2.7.5",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^4.5.3",
    "postcss-cli": "^4.1.0"
  }
}
